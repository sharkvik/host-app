{
	"name": "app-host",
	"version": "1.0.0",
	"scripts": {
		"ng": "ng",
		"start": "npm run build-all && ng serve --port 3001",
		"start:prod": "npm run build-all:prod && ng serve --port 3001",
		"build": "ng build",
		"build:prod": "npm run build-all:prod && ng build --prod",
		"test": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"build:placeholder-plugin": "ng build --project placeholder-plugin --output-hashing none",
		"build:placeholder-plugin:prod": "ng build --prod --project placeholder-plugin --output-hashing none",
		"copy:placeholder-plugin": "cpr dist/placeholder-plugin/main.js src/assets/plugins/placeholder-plugin.bundle.js -o",
		"build:data-plugin": "ng build --project data-plugin --output-hashing none",
		"build:data-plugin:prod": "ng build --prod --project data-plugin --output-hashing none",
		"copy:data-plugin": "cpr dist/data-plugin/main.js src/assets/plugins/data-plugin.bundle.js -o",
		"build-all": "npm run build:placeholder-plugin && npm run copy:placeholder-plugin && npm run build:data-plugin && npm run copy:data-plugin",
		"build-all:prod": "npm run build:placeholder-plugin:prod && npm run copy:placeholder-plugin && npm run build:data-plugin:prod && npm run copy:data-plugin"
	},
	"private": true,
	"dependencies": {
		"@angular/common": "^6.0.3",
		"@angular/core": "^6.0.3",
		"@angular/compiler": "^6.0.3",
		"@angular/animations": "^6.0.3",
		"@angular/elements": "^6.1.6",
		"@angular/forms": "^6.0.3",
		"@angular/http": "^6.0.3",
		"@angular/platform-browser": "^6.0.3",
		"@angular/platform-browser-dynamic": "^6.0.3",
		"@angular/router": "^6.0.3",
		"@webcomponents/custom-elements": "^1.2.0",
		"core-js": "^2.5.4",
		"document-register-element": "^1.7.2",
		"lodash": "4.17.10",
		"rxjs": "^6.0.0",
		"zone.js": "^0.8.26"
	},
	"devDependencies": {
		"@angular/compiler-cli": "^6.0.3",
		"@angular-devkit/build-ng-packagr": "~0.6.8",
		"@angular-devkit/build-angular": "0.7.5",
		"ng-packagr": "^3.0.0-rc.2",
		"tsickle": ">=0.25.5",
		"tslib": "^1.7.1",
		"typescript": "~2.7.2",
		"@angular/cli": "~6.0.8",
		"@angular/language-service": "^6.0.3",
		"@types/jasmine": "~2.8.6",
		"@types/jasminewd2": "~2.0.3",
		"@types/node": "~8.9.4",
		"codelyzer": "~4.2.1",
		"jasmine-core": "~2.99.1",
		"jasmine-spec-reporter": "~4.2.1",
		"karma": "~1.7.1",
		"karma-chrome-launcher": "~2.2.0",
		"karma-coverage-istanbul-reporter": "~2.0.0",
		"karma-jasmine": "~1.1.1",
		"karma-jasmine-html-reporter": "^0.2.2",
		"ngx-build-plus": "^6.1.0",
		"protractor": "~5.3.0",
		"ts-node": "~5.0.1",
		"tslint": "~5.9.1"
	}
}
